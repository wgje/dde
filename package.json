{
  "name": "nanoflow-project-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run config && ng serve",
    "build": "npm run config && rm -rf dist .angular && ng build",
    "build:strict": "node scripts/validate-env.cjs --production && npm run config && rm -rf dist .angular && ng build",
    "build:dev": "npm run config && ng build",
    "watch": "ng build --watch --configuration development",
    "validate-env": "node scripts/validate-env.cjs",
    "validate-env:prod": "node scripts/validate-env.cjs --production",
    "test": "vitest",
    "test:run": "vitest run",
    "test:run:pure": "vitest run --config vitest.pure.config.mts",
    "test:run:services": "vitest run --config vitest.services.config.mts",
    "test:run:components": "vitest run --config vitest.components.config.mts",
    "test:run:split": "npm run test:run:services && npm run test:run:components",
    "test:run:split3": "npm run test:run:pure && npm run test:run:services && npm run test:run:components",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint 'src/**/*.ts' --no-warn-ignored",
    "lint:fix": "eslint 'src/**/*.ts' --fix --no-warn-ignored",
    "config": "node scripts/set-env.cjs",
    "seed:supabase": "node scripts/seed-supabase.js",
    "setup:storage": "node scripts/setup-storage-bucket.cjs"
  },
  "dependencies": {
    "@angular/build": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.2.17",
    "@angular/service-worker": "^19.0.0",
    "@sentry/angular": "^10.32.1",
    "@sentry/node": "^10.32.1",
    "@supabase/supabase-js": "^2.84.0",
    "dotenv": "^17.2.3",
    "gojs": "^3.1.1",
    "rxjs": "^7.8.2",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@testing-library/dom": "^10.4.1",
    "@types/jasmine": "^5.1.13",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-rxjs-x": "^0.8.3",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "jasmine-core": "^5.12.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "postcss": "^8.5.6",
    "shadcn": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.0",
    "vite": "^6.2.0",
    "vitest": "^4.0.14"
  }
}

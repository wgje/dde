{
  "name": "nanoflow-project-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.19.0 <24"
  },
  "scripts": {
    "ng": "node scripts/run-ng.cjs",
    "start": "npm run config && NODE_OPTIONS=--max_old_space_size=4096 NG_BUILD_MAX_WORKERS=1 NG_BUILD_PARALLEL_TS=0 NG_BUILD_TYPE_CHECK=0 ESBUILD_WORKER_THREADS=0 node scripts/run-ng.cjs serve",
    "build": "npm run config && rm -rf dist .angular && NODE_OPTIONS=--max_old_space_size=4096 NG_BUILD_MAX_WORKERS=1 NG_BUILD_PARALLEL_TS=0 NG_BUILD_TYPE_CHECK=0 ESBUILD_WORKER_THREADS=0 node scripts/run-ng.cjs build && node scripts/inject-modulepreload.cjs",
    "build:strict": "node scripts/validate-env.cjs --production && npm run config && rm -rf dist .angular && NODE_OPTIONS=--max_old_space_size=4096 NG_BUILD_MAX_WORKERS=2 NG_BUILD_PARALLEL_TS=1 NG_BUILD_TYPE_CHECK=1 ESBUILD_WORKER_THREADS=0 node scripts/run-ng.cjs build && node scripts/inject-modulepreload.cjs",
    "build:dev": "npm run config && NODE_OPTIONS=--max_old_space_size=4096 NG_BUILD_MAX_WORKERS=1 NG_BUILD_PARALLEL_TS=0 NG_BUILD_TYPE_CHECK=0 ESBUILD_WORKER_THREADS=0 node scripts/run-ng.cjs build",
    "watch": "node scripts/run-ng.cjs build --watch --configuration development",
    "validate-env": "node scripts/validate-env.cjs",
    "validate-env:prod": "node scripts/validate-env.cjs --production",
    "test": "vitest",
    "test:contracts": "node scripts/contracts/check-flow-current-userid.cjs",
    "test:run": "npm run test:run:fast",
    "test:run:fast": "npm run test:contracts && node scripts/run-test-matrix.cjs --mode=local --max-procs=auto --quarantine=scripts/test-quarantine.json",
    "test:run:full": "npm run test:contracts && node scripts/run-test-matrix.cjs --mode=local --max-procs=auto --strategy=mod --include-quarantine --quarantine=scripts/test-quarantine.json",
    "test:run:ci": "npm run test:contracts && node scripts/run-test-matrix.cjs --mode=ci --max-procs=auto --strategy=weighted --include-quarantine --quarantine=scripts/test-quarantine.json",
    "test:run:legacy": "vitest run --exclude src/tests/perf/**",
    "test:run:ci-shard": "npm run test:run:ci",
    "test:run:minimal": "node scripts/run-test-matrix.cjs --lane=lane_node_minimal,lane_browser_minimal",
    "test:run:testbed": "node scripts/run-test-matrix.cjs --lane=lane_testbed_service,lane_testbed_component",
    "test:run:related": "vitest related --run",
    "test:run:stress": "node scripts/stress/run-repeat.cjs --rounds=10",
    "test:run:stress:lane": "node scripts/stress/run-repeat.cjs",
    "test:run:contention": "node scripts/stress/run-contention-pair.cjs",
    "test:run:pure": "vitest run --config vitest.pure.config.mts",
    "test:run:services": "vitest run --config vitest.services.config.mts",
    "test:run:components": "vitest run --config vitest.components.config.mts",
    "test:run:split": "npm run test:run:services && npm run test:run:components",
    "test:run:split3": "npm run test:run:pure && npm run test:run:services && npm run test:run:components",
    "test:coverage": "vitest run --coverage",
    "test:perf": "vitest run --config vitest.perf.config.mts",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint 'src/**/*.ts' --no-warn-ignored",
    "lint:fix": "eslint 'src/**/*.ts' --fix --no-warn-ignored",
    "config": "node scripts/set-env.cjs",
    "postinstall": "node scripts/patch-esbuild.cjs",
    "db:types": "npx supabase gen types typescript --linked > src/types/supabase.ts",
    "seed:supabase": "node scripts/seed-supabase.js",
    "setup:storage": "node scripts/setup-storage-bucket.cjs",
    "analyze:bundle": "bash scripts/analyze-bundle.sh",
    "perf:benchmark": "bash scripts/performance-benchmark.sh",
    "perf:guard:nojit": "node scripts/check-main-no-jit.cjs",
    "perf:guard:startup": "node scripts/perf-startup-guard.cjs",
    "perf:guard:no-regression": "node scripts/perf-no-regression-guard.cjs",
    "quality:guard:font-contract": "node scripts/check-font-subset-contract.cjs",
    "quality:guard:supabase-ready": "node scripts/check-supabase-ready-contract.cjs",
    "perf:guard": "npm run build:dev -- --stats-json && node scripts/inject-modulepreload.cjs && npm run perf:guard:nojit && npm run perf:guard:startup && npm run quality:guard:font-contract && npm run quality:guard:supabase-ready",
    "test:baseline:update": "node scripts/update-test-duration-baseline.cjs --baseline=scripts/test-duration-baseline.json --quarantine=scripts/test-quarantine.json",
    "test:quarantine:update": "node scripts/update-test-quarantine.cjs --quarantine=scripts/test-quarantine.json"
  },
  "dependencies": {
    "@angular/common": "^19.2.18",
    "@angular/compiler": "^19.2.18",
    "@angular/core": "^19.2.18",
    "@angular/forms": "^19.2.18",
    "@angular/platform-browser": "^19.2.18",
    "@angular/platform-browser-dynamic": "^19.2.18",
    "@angular/router": "^19.2.18",
    "@angular/service-worker": "^19.2.18",
    "@sentry/angular": "^10.32.1",
    "@supabase/supabase-js": "^2.84.0",
    "dompurify": "^3.3.1",
    "gojs": "^3.1.1",
    "idb-keyval": "^6.2.2",
    "rxjs": "^7.8.2",
    "web-vitals": "^5.1.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^19.2.18",
    "@angular/cli": "^19.2.18",
    "@angular/compiler-cli": "^19.2.18",
    "@playwright/test": "^1.48.0",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "dotenv": "^17.2.3",
    "esbuild": "0.25.4",
    "eslint": "^9.39.1",
    "fake-indexeddb": "^6.0.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.0",
    "vite": "^6.2.0",
    "vitest": "^4.0.14"
  },
  "overrides": {
    "esbuild": "0.25.4"
  }
}
